<html>
    <head>
        <!-- Make site responsive on mobile/tablet -->
        <link rel="icon" type="https://www.google.co.in/imgres?imgurl=https%3A%2F%2Fd33wubrfki0l68.cloudfront.net%2F06c5eb42ff383f64fa46b1f53797349fa0a8c595%2F9e0e7%2Fimages%2Flarge_bitmapicons%2Farrows%2Farrow-y.png&imgrefurl=https%3A%2F%2Fwww.app-bits.com%2Ficon%2Farrow-y%2F&docid=DWAGx72mZwLQAM&tbnid=ZOz_lYjjBX4X5M%3A&vet=10ahUKEwjBsPbcrrjeAhVDQH0KHSIYBR4QMwhhKBgwGA..i&w=960&h=960&bih=678&biw=1301&q=stylish%20y%20images&ved=0ahUKEwjBsPbcrrjeAhVDQH0KHSIYBR4QMwhhKBgwGA&iact=mrc&uact=8 " />
        
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>YelpCamp</title>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <link rel="stylesheet" href="/stylesheets/main.css">
        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-115501645-1"></script>
        <script>
           window.dataLayer = window.dataLayer || [];
           function gtag(){dataLayer.push(arguments);}
           gtag('js', new Date());
           gtag('config', 'UA-115501645-1');
        </script>
        <!-- Make site responsive on mobile/tablet -->
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <script src='https://www.google.com/recaptcha/api.js'></script>
    </head>
    
    <body>
    <nav class="navbar navbar-default">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>

            <a class="navbar-brand" href="/">YelpCamp</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="<%= typeof page !== 'undefined' && page === 'campgrounds' ? 'active' : '' %>"><a href="/campgrounds">Home</a></li>
          </ul>
          <ul class="nav navbar-nav navbar-right">
              <% if(!currentUser){ %>
                  <li class="<%= typeof page !== 'undefined' && page === 'login' ? 'active' : '' %>"><a href="/login">Login</a></li>
                  <li class="<%= typeof page !== 'undefined' && page === 'register' ? 'active' : '' %>"><a href="/register">Sign Up</a></li>
              <% } else { %>
                  <li><a href="#">Signed In As <%= currentUser.username %></a></li>
                  <li><a href="/resetnotforget">Reset Password</a></li>
                  <li><a href="/logout">Logout</a></li>
              <% } %>
          </ul>
        </div>
    </div>
</nav>
    
    <div class="container">
        
        <% if(error && error.length > 0){ %>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
        <% } %>
        <% if(success && success.length > 0){ %>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
        <% } %>
    </div>
    
